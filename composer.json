{
  "name": "procurios/json-rpc",
  "description": "JSON-RPC 2.0 server in PHP",
  "keywords": [
    "json",
    "rpc",
    "json-rpc",
    "server",
    "psr",
    "psr-7"
  ],
  "homepage": "https://github.com/procurios/JsonRpc",
  "license": "MIT",
  "require": {
    "php": ">=5.6",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": ">=5-stable"
  },
  "autoload": {
    "psr-4": {
      "Procurios\\Json\\JsonRpc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Procurios\\Json\\JsonRpc\\test\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
